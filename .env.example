# API Configuration
# Set VITE_API_URL to use the new SurrealDB-backed API
# Leave unset to use legacy Supabase Edge Function
VITE_API_URL=http://localhost:8787

# Legacy Supabase Configuration (can be removed after full migration)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_PUBLISHABLE_KEY=your-anon-key

# SurrealDB Configuration (for local development)
VITE_SURREALDB_WS_URL=ws://localhost:8000/rpc
VITE_SURREALDB_HTTP_URL=http://localhost:8000
VITE_SURREALDB_NAMESPACE=musiqasik
VITE_SURREALDB_DATABASE=main
VITE_SURREALDB_USER=root
VITE_SURREALDB_PASS=root

# Last.fm API
VITE_LASTFM_API_KEY=your_lastfm_api_key
LASTFM_API_KEY=your_lastfm_api_key
