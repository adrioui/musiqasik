{
  "name": "musiqasik",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite dev",
    "dev:worker": "wrangler dev",
    "dev:full": "concurrently -n worker,web -c cyan,green \"bun run dev:worker\" \"bun run dev\"",
    "build": "bunx --bun vite build && tsc --noEmit",
    "preview": "bun run build && wrangler dev",
    "deploy": "bun run build && wrangler deploy",
    "deploy:staging": "bun run build && wrangler deploy --env staging",
    "deploy:prod": "bun run build && wrangler deploy --env production",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "typecheck:worker": "tsc -p tsconfig.worker.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "@effect/platform": "^0.94.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "baseline-browser-mapping": "^2.9.11",
    "caniuse-lite": "^1.0.30001762",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "effect": "^3.19.13",
    "hono": "^4.11.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "overrides": {
    "caniuse-lite": "^1.0.30001762"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@cloudflare/workers-types": "^4.20251231.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.1",
    "@types/bun": "^1.3.5",
    "@types/d3": "^7.4.3",
    "@types/node": "^22.16.5",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "jsdom": "^27.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.21.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.16",
    "wrangler": "^4.54.0"
  }
}
