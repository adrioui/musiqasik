{
  "name": "musiqasik",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "dev:server": "bun run --hot server/index.ts",
    "dev:full": "concurrently -n server,web -c cyan,green \"bun run dev:server\" \"bun run dev\"",
    "build": "vite build && tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "dev:db": "docker run --rm --name musiqasik-db -v ./surrealdb-data:/data -p 8000:8000 surrealdb/surrealdb start --user root --pass root file:/data/musiqasik.db",
    "dev:all": "concurrently -n db,server,web -c blue,cyan,green \"bun run dev:db\" \"bun run dev:server\" \"bun run dev\"",
    "wasm:build": "cd rust/graph-wasm && chmod +x build.sh && ./build.sh",
    "wasm:build:release": "cd rust/graph-wasm && wasm-pack build --target web --release --out-dir ../../src/wasm/pkg",
    "wasm:clean": "rm -rf src/wasm/pkg",
    "wasm:check": "cd rust/graph-wasm && cargo check",
    "wasm:test": "cd rust/graph-wasm && cargo test",
    "prebuild": "npm run wasm:build:release || echo 'WASM build skipped (wasm-pack not available)'",
    "dev:wasm": "VITE_USE_WASM_GRAPH=true VITE_WASM_DEBUG=true npm run dev"
  },
  "dependencies": {
    "@effect/platform": "^0.94.0",
    "@effect/platform-bun": "^0.87.0",
    "@effect/platform-node": "^0.104.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "baseline-browser-mapping": "^2.9.11",
    "caniuse-lite": "^1.0.30001761",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "effect": "^3.19.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "surrealdb": "^1.3.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.1",
    "@types/bun": "^1.3.5",
    "@types/d3": "^7.4.3",
    "@types/node": "^22.16.5",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^15.15.0",
    "jsdom": "^27.3.0",
    "postcss": "^8.5.6",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.21.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.0.0",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.16"
  }
}
