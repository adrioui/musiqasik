{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "types": ["@cloudflare/workers-types", "node"],
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["worker/**/*.ts"],
  "exclude": ["node_modules"]
}
